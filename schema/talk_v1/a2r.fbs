enum A2rErrorType: uint8 {
  ErrorUnspecified = 0,
  ErrorInvalidData = 1,
}

table A2rError {
  error_type: A2rErrorType;
  error_message: string;
}

table A2rToFrontend {
  frontend_id: uint64;
  payload: [uint8];
}

union A2rRootPayload {
  Error: A2rError = 1,
  ToFrontend: A2rToFrontend = 2,
}

table A2rRoot {
  root_payload: A2rRootPayload;
  message_id: uint64;
}
