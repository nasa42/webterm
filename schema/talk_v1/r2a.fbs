enum R2aErrorType: uint8 {
  ErrorUnspecified = 0,
}

table R2aError {
  error_type: R2aErrorType;
}

table R2aFromFrontend {
  frontend_id: uint64;
  payload: [uint8];
}

union R2aRootPayload {
  Error: R2aError = 1,
  FromFrontend: R2aFromFrontend = 2,
  RelayShuttingDown: EmptyTable = 4,
}

table R2aRoot {
  root_payload: R2aRootPayload;
}
